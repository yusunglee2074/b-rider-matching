# ===========================================
# B-Rider Development Environment Variables
# ===========================================
# Copy this file to .env and fill in the values

# Database (PostgreSQL)
DATABASE_URL=postgresql://brider:brider_dev_password@localhost:5432/brider

# Redis (Cache, Lock, Geohash, BullMQ)
REDIS_URL=redis://localhost:6379

# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRATION=3600

# Service URLs (for inter-service communication)
CORE_SERVICE_URL=http://localhost:3001
LOCATION_SERVICE_URL=http://localhost:3003

# gRPC
LOCATION_GRPC_URL=localhost:5003

# Node Environment
NODE_ENV=development

# Logging
LOG_LEVEL=debug
